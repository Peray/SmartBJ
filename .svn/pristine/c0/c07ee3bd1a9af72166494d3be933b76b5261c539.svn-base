@charset "UTF-8";

/* maki */

[class*="maki-"]:before {
    font-family: 'maki', sans-serif;
}


/* entypo */

[class*="entypo-"]:before {
    font-family: 'entypo', sans-serif;
}


/* fontawesome */

[class*="fontawesome-"]:before {
    font-family: 'FontAwesome', sans-serif;
}

body {
    color: #9ea7b3!important;
    font-family: "微软雅黑"!important;
    font-size: 13px!important;
    line-height: 20px;
    min-height: 100%;
    z-index: -2;
    margin: 0px !important;
    background: url('../img/home/bg.png') no-repeat top center fixed;
    -moz-background-size: cover;
    -webkit-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    overflow: hidden;
}


a,
a:hover {
    text-decoration: none!important;
    color: #000;
}

ul,
ol {
    list-style: none;
}

* {
    margin: 0;
    padding: 0;
}

.clearfix:after {
    content: "";
    display: block;
    clear: both;
    height: 0;
}

.clearfix {
    zoom: 1;
}
clearfix>li{
	float:left;	
}
input,
textarea {
    border: none;
}

.error {
    padding: 0 10px;
    color: red;
    font-size: 14px;
}

.code {
    background-image: url(code.jpg);
    font-family: Arial;
    font-style: italic;
    color: red;
    border: 0;
    padding: 0px 3px;
    letter-spacing: 3px;
    font-weight: bolder;
    text-align: center;
}


/*登陆注册开始*/
 .inmap {
     width: 260px;
     height: 30px;
     text-indent: 1em;
     border: 1px solid #ccc;
     border-radius: 3px;
     background-color: #fff;
 }
 
 .input_sub {
     width: 160px;
     height: 30px;
     line-height: 30px;
     background-color: #2795dc;
     border-bottom: 3px solid #0078b3;
     border-radius: 2px;
     color: #fff;
     text-align: center;
     border: none;
     margin: 0 20px;
     cursor: pointer;
 }
 .p_password{
     height:auto!important;
 }
 .p_xieyi{
    height:auto!important;
    margin-left:55px!important;
 }
 .input_xieyi{
    vertical-align: middle;
 }
.xieyi{
    font-size: 12px;
    text-align:left!important;
    width:auto!important;
}
a.xieyi{
    color:#2795dc;
}
a.xieyi:hover{
    text-decoration: underline!important;
}
/*登陆注册结束*/
